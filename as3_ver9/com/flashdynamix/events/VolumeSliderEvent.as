package com.flashdynamix.events {	import flash.events.Event;	/**	 * @author FlashDynamix	 */	public class VolumeSliderEvent extends Event {		public static const ON_CHANGE : String = "slider_on_change";		public var position : Number = 0;		public function VolumeSliderEvent(type : String, position : Number = 0, bubbles : Boolean = false, cancelable : Boolean = false) {			this.position = position;			super(type, bubbles, cancelable);		}		override public function clone() : Event {			return new VolumeSliderEvent(type, position);		}	}}