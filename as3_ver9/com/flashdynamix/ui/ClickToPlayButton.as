package com.flashdynamix.ui {	import com.flashdynamix.abstract.AbstractButton;	import com.flashdynamix.motion.Tweensy;		import flash.events.MouseEvent;		/**	 * @author FlashDynamix	 */	public class ClickToPlayButton extends AbstractButton {		public function ClickToPlayButton() {			super();						this.alpha = 0.9;		}		override protected function mouseOver(event : MouseEvent) : void {			if(!_enabled) return;						Tweensy.to(this, {alpha:1}, 1);		}		override protected function mouseOut(event : MouseEvent) : void {			if(!_enabled) return;						Tweensy.to(this, {alpha:0.9}, 1);		}	}}