package com.flashdynamix.events {	import flash.events.Event;		/**	 * @author FlashDynamix	 */	public class FLVEvent extends Event {		public static const LOADED : String = "flv_loaded";		public static const BUFFER_EMPTY : String = "flv_buffer_empty";		public static const BUFFER_FULL : String = "flv_buffer_full";		public static const PLAY_START : String = "flv_play_start";		public static const SEEKED : String = "flv_seeked";		public static const META_LOADED : String = "flv_meta_loaded";		public static const PLAY_COMPLETE : String = "flv_play_complete";		public function FLVEvent(type : String, bubbles : Boolean = false, cancelable : Boolean = false) {			super(type, bubbles, cancelable);		}		override public function clone() : Event {			return new FLVEvent(type, bubbles, cancelable);		}	}}