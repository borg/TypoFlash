package com.flashdynamix.events {	import flash.events.Event;		/**	 * @author FlashDynamix	 */	public class VideoControlEvent extends Event {		public static const ON_SEEK : String = "on_seek";		public static const ON_PAUSE : String = "on_pause";		public static const ON_PLAY : String = "on_play";		public static const ON_VOLUME_CHANGE : String = "on_volume_change";		public static const ON_FULL_SCREEN : String = "on_full_screen";		public function VideoControlEvent(type : String, bubbles : Boolean = false, cancelable : Boolean = false) {			super(type, bubbles, cancelable);		}		override public function clone() : Event {			return new VideoControlEvent(type, bubbles, cancelable);		}	}}